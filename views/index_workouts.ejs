<html>
    <body>
        <h1>Workout Dashboard</h1>
        
        <h2> Workout history</h2>
        
        <div>
            <ul>
                <% for (var i = 0; i < tenndr.length; i++) { %>
                    <li>
                        <a href="/tenndr/<%= tenndr[i]._id %>">
                            <%= tenndr[i].workout %>
                        </a>
                    </li>
                <% } %>
            </ul>
        </div>
        <div>
          <h2>Post Workout Mood</h2>
            <ul>
              <% for (var i = 0; i < tenndr.length; i++) { %>
                  <li>
                      <a href="/tenndr/<%= tenndr[i]._id %>">
                          <%= tenndr[i].mood_after %>
                      </a>
                  </li>
                  <% } %>
            </ul>


      <a class ="waves-effect amber waves-light btn" href="/tenndr/new"><span class="purple-text darken-3">Add a workout</span></a>

  </body>

</html>