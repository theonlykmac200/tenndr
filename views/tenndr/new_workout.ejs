    <body>

        <h1>New Workout</h1>
        <div>
            <form action="/tenndr" method="POST">
                <span class="bolded">workout:</span><input type="text" name="workout" /><br />
                <span class="bolded">Description:</span><input type="text" name="description" /><br />
                <span class="bolded">duration:</span><input type="number" name="duration" /><br />
                <span class="bolded">Mood_before:</span><input type="text" name="mood_before" /><br />
                <span class="bolded">Mood_after:</span><input type="text" name="mood_after" /><br />
